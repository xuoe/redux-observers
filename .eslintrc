{
    "extends": "eslint:recommended",
    "rules": {
        "array-bracket-spacing": [2, "never"],
        "eqeqeq": [1, "smart"]
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "sourceType": "module"
    }
}
